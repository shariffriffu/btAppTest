                "type": "TextField",
                "id": "createPass",
                "hintText": "Enter App Pass",
                "keyboardType": "phone",
                "fontColor": "#124682",
                "textFieldPropertyType": "typeA",
                "label": "Create App Pass",
                "isPassword": true,
                "maxLength": 4
