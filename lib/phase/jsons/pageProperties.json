            {
                "type": "SizedBox",
                "heightPercent": 8
            },
            {
                "type": "Button",
                "label": "Register",
                "buttonStyleType": "typeD",
                "onPressed": "register"
            },
            {
                "type": "SizedBox",
                "heightPercent": 18
            },
            {
                "type": "RichText",
                "textPrefix": "Already have an account? ",
                "textAction": "Login",
                "onTapAction": "login",
                "fontSize": 14,
                "fontWeight": "w600",
                "fontColor": "#1C3144",
                "OnPressfontSize": 17,
                "OnPressfontWeight": "bold",
                "OnPressfontColor": "#F47A27"
            }
        ],
        "validation": {
            "mobile": {
                "regex": "^[0-9]{8}$",
                "errorMessage": "Please enter a valid mobile number."
            },
            "createPass": {
                "regex": "^[0-9]{4}$",
                "errorMessages": {
                    "empty": "Please enter your password.",
                    "tooShort": "Password must be 4 digits.",
                    "invalidFormat": "Password must contain only digits."
                }
            },
            "confirmPass": {
                "matchField": "createPass",
                "errorMessage": "Passwords do not match."
            }
        }
    },
