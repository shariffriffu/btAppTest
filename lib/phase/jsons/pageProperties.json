{
    "login": {
        "layout": "Column",
        "children": [
            {
                "type": "SizedBox",
                "heightPercent": 20
            },
            {
                "type": "PopUpMenu",
                "alignment": "end",
                "color": "#F37B26",
                "child": {
                    "type": "PopupMenu"
                }
            },
            {
                "type": "Image",
                "asset": "lib/phase/images/logo.png",
                "height": 100
            },
            {
                "type": "TextField",
                "id": "mobile",
                "label": "Mobile Number",
                "textFieldPropertyType": "typeA",
                "hintText": "Enter Mobile Number",
                "keyboardType": "phone",
                "maxLength": 8,
                "fontColor": "#1C3144",
                "fontWeight": "bold"
            },
            {
                "type": "SizedBox",
                "heightPercent": 8
            },
            {
                "type": "TextField",
                "id": "password",
                "hintText": "App Pass",
                "keyboardType": "phone",
                "fontColor": "#124682",
                "textFieldPropertyType": "typeA",
                "label": "App Pass",
                "isPassword": true,
                "maxLength": 4
            },
            {
                "type": "GestureDetectorText",
                "text": "Forgot App Pass?",
                "alignment": "end",
                "onTapAction": "forgotPassword",
                "fontSize": 14,
                "maxLength": 4,
                "fontWeight": "bold",
                "textFieldPropertyType": "typeB"
            },
            {
                "type": "SizedBox",
                "heightPercent": 8
            },
            {
                "type": "Button",
                "label": "Login",
                "buttonStyleType": "typeA",
                "onPressed": "login"
            },
            {
                "type": "SizedBox",
                "heightPercent": 8
            },
            {
                "type": "GestureDetectorImage",
                "asset": "lib/phase/images/finger.png",
                "height": 60,
                "width": 60,
                "onTapAction": "fingerPrint",
                "biometric": true
            },
            {
                "type": "lineSaparate",
                "text": "Or sign in with",
                "dividerColor": "#1C3144",
                "textFieldPropertyType": "typeB"
            },
            {
                "type": "ButtonInRow",
                "NdiButtonColor": "#5AC994",
                "image": "lib/phase/images/NDILogo.png",
                "textFieldPropertyType": "typeC",
                "height": 20,
                "width": 20,
                "alignment": "center",
                "text": "Login with Bhutan NDI",
                "onTapAction": "LoginWithNdi"
            },
            {
                "type": "SizedBox",
                "heightPercent": 18
            },
            {
                "type": "RichText",
                "textPrefix": "Don’t have an Account? ",
                "textAction": " Register",
                "onTapAction": "register",
                "fontSize": 14,
                "fontWeight": "w600",
                "fontColor": "#1C3144",
                "OnPressfontSize": 17,
                "OnPressfontWeight": "bold",
                "OnPressfontColor": "#F47A27"
            }
        ],
        "validation": {
            "mobile": {
                "regex": "^[0-9]{8}$",
                "errorMessage": "Please enter a valid mobile number."
            },
            "password": {
                "regex": "^[0-9]{4}$",
                "errorMessages": {
                    "empty": "Please enter your password.",
                    "tooShort": "Password must be 4 digits.",
                    "invalidFormat": "Password must contain only digits."
                }
            }
        }
    },
    "OtpPage": {
        "layout": "Column",
        "children": [
            {
                "type": "SizedBox",
                "heightPercent": 20
            },
            {
                "type": "PopUpMenu",
                "alignment": "end",
                "color": "#F37B26",
                "child": {
                    "type": "PopupMenu"
                }
            },
            {
                "type": "Image",
                "asset": "lib/phase/images/logo.png",
                "height": 140
            },
            {
                "type": "Label",
                "text": "OTP Verification",
                "padding": {
                    "left": 20,
                    "right": 50
                },
                "alignment": "Start",
                "fontColor": "#1C3144",
                "fontWeight": "w600",
                "fontSize": 16
            },
            {
                "type": "SizedBox",
                "heightPercent": 10
            },
            {
                "type": "Label",
                "text": "We have sent the verification code to your phone number",
                "padding": {
                    "left": 20,
                    "right": 50
                },
                "alignment": "Start",
                "fontColor": "#1C3144",
                "fontWeight": "normal",
                "fontSize": 14
            },
            {
                "type": "SizedBox",
                "heightPercent": 20
            },
            {
                "type": "otpField",
                "id": "forgotPinOtp"
            },
            {
                "type": "SizedBox",
                "heightPercent": 30
            },
            {
                "type": "RowButton",
                "mainAxisAlignment": "spaceEvenly",
                "buttons": [
                    {
                        "label": "Resend OTP",
                        "buttonStyleType": "typeB",
                        "onTapAction": "resend"
                    },
                    {
                        "label": "Proceed",
                        "buttonStyleType": "typeC",
                        "onTapAction": "proceed"
                    }
                ]
            }
        ]
    },
